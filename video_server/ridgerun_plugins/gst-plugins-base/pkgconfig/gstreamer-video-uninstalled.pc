# the standard variables don't make sense for an uninstalled copy
prefix=
exec_prefix=
libdir=@videolibdir@
# includedir is builddir because it is used to find gstconfig.h in places
includedir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs
girdir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs/gst/video
typelibdir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs/gst/video

Name: GStreamer Video Library, Uninstalled
Description: Video base classes and helper functions, uninstalled
Version: 1.14.5
Requires: gstreamer-1.0 gstreamer-base-1.0
Requires.private: orc-0.4
Libs: -L${libdir} -lgstvideo-1.0
Cflags: -I/video_server/ridgerun_plugins/gst-plugins-base/gst-libs -I/video_server/ridgerun_plugins/gst-plugins-base/gst-libs

