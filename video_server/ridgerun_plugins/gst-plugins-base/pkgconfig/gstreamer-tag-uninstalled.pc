# the standard variables don't make sense for an uninstalled copy
prefix=
exec_prefix=
libdir=@taglibdir@
# includedir is builddir because it is used to find gstconfig.h in places
includedir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs
girdir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs/gst/tag
typelibdir=/video_server/ridgerun_plugins/gst-plugins-base/gst-libs/gst/tag

Name: GStreamer Tag Library, Uninstalled
Description: Tag base classes and helper functions, uninstalled
Version: 1.14.5
Requires: gstreamer-1.0
Libs: -L${libdir} -lgsttag-1.0
Libs.Private: -lz
Cflags: -I/video_server/ridgerun_plugins/gst-plugins-base/gst-libs -I/video_server/ridgerun_plugins/gst-plugins-base/gst-libs

