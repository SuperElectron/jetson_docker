# the standard variables don't make sense for an uninstalled copy
prefix=
exec_prefix=
libdir=/video_server/ridgerun_plugins/gstreamer/gst/.libs
# includedir is builddir because it is used to find gstconfig.h in places
includedir=/video_server/ridgerun_plugins/gstreamer
toolsdir=/video_server/ridgerun_plugins/gstreamer/tools
pluginsdir=/video_server/ridgerun_plugins/gstreamer
girdir=/video_server/ridgerun_plugins/gstreamer/gst
bashhelpersdir=/video_server/ridgerun_plugins/gstreamer/data/bash-completion/helpers
helpersdir=/video_server/ridgerun_plugins/gstreamer/libs/gst/helpers
typelibdir=/video_server/ridgerun_plugins/gstreamer/gst

Name: GStreamer Uninstalled
Description: Streaming media framework, Not Installed
Version: 1.14.5
Requires: glib-2.0, gobject-2.0
Requires.private: gmodule-no-export-2.0  
Libs: -L${libdir} -lgstreamer-1.0
Cflags: -I/video_server/ridgerun_plugins/gstreamer -I/video_server/ridgerun_plugins/gstreamer/libs -I/video_server/ridgerun_plugins/gstreamer -I/video_server/ridgerun_plugins/gstreamer/libs
